## CoreUtils
alias cl="toybox clear"
alias cp="toybox cp -ip"
alias cr="toybox cp -ir"
alias df="toybox df -h"
alias di="\df -hi"
alias dit="\dig +noall +nocmd +answer +multiline"
alias du="toybox du -hs ./*"
alias free="toybox free -h"
alias hurl="\curl -f#LO"
alias l="toybox ls -hSCF --color=auto"
alias la="toybox ls -haSCF --color=auto"
alias ll="toybox ls -halSF --color=auto"
alias ls="toybox ls -hSCF --color=auto"
alias mkdir="toybox mkdir -vp $@"
alias mktar="\tar -zcvf archive_`date +%Y%m%d`-`date +%H%M%S`.tar.gz"
alias more="\less"
alias mv="toybox mv -i"
alias nano="\nano -EOSWcimx"
alias pss="toybox ps -af | toybox head -1; toybox ps -af | toybox grep -v grep"
alias psw="toybox watch 'toybox ps -af | sort -rk4'"
alias rf="toybox rm -rf"
alias rm="toybox rm -i"
alias rr="toybox rm -ir"
alias screen="\screen"
alias ss="\scrot -s ~/Pictures/'Screenshot-%Y%m%d'.png"
alias untar="\tar -vxf"
